<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vertial.fivemiov">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config"
        >

        <activity android:name="com.vertial.fivemiov.ui.RegistrationAuthorization.RegistrationAuthorizationActivity"
                    android:theme="@style/RegistrationTheme"
                    android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.vertial.fivemiov.ui.webview_activity.WebViewActivity" />

        <activity
            android:name="com.vertial.fivemiov.ui.MainActivity"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>